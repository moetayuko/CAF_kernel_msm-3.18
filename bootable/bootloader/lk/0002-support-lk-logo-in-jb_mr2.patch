@@ -0,0 +1,44 @@
+#include <debug.h>
+#include <hw_boardid_self_adapt.h>
+#include <smem.h>
+
+uint32 hw_boardid = 0;
+/*
+ * get_boardid_from_sharemem ---- get board id from share memery
+ * returns
+ *   board id
+ */
+int get_boardid_from_sharemem()
+{
+	unsigned smem_status;
+	int boardid = 0;
+	hw_boardid_info_type boardid_info;
+	memset((void*)&boardid_info, 0, sizeof(hw_boardid_info_type));
+
+	/* get allocated shared memory */
+	smem_status = smem_read_alloc_entry(SMEM_HW_SW_BUILID_ID, &boardid_info, sizeof(hw_boardid_info_type));
+	dprintf(INFO, "board_id0:%d,board_id1:%d\n",boardid_info.board_id0,boardid_info.board_id1);
+	if (smem_status)
+	{
+		dprintf(CRITICAL, "get shared memory failed\n");
+		return HW_BOARDID_FAIL;
+	}
+	/* set baord id */
+	boardid = boardid_info.board_id0;
+	boardid = boardid << 4;
+	boardid |= boardid_info.board_id1;
+	return boardid&HW_BOARDID_MASK;
+}
+
+/*
+ *set_boardid_config ---- set board id for selectting dts
+ */
+void boardid_init()
+{
+	hw_boardid = get_boardid_from_sharemem();
+	if(HW_BOARDID_FAIL == hw_boardid)
+	{
+		dprintf(CRITICAL, "set board id failed\n");
+	}
+	return;
+}
